                    "command": "appService.Deploy",
                    "when": "explorerResourceIsFolder == false && resourceExtname =~ /.[j|w]ar$/",
                    "group": "zzz_azuretools_deploy@1"
                },
                {
                    "command": "appService.appSettings.Upload",
